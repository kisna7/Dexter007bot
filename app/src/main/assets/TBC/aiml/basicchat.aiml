<aiml>
        
	<category>
        <pattern>HELLO BOT</pattern>
        <template>HI, CAN YOU HELP ME TO GET INFORMATION ABOUT YOUR REQUIREMENT?</template>
    </category>
    <category>
        <pattern>YES</pattern>
